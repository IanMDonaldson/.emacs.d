(custom-set-faces
 ;; custom-set-faces was added by Custom.
 ;; If you edit it by hand, you could mess it up, so be careful.
 ;; Your init file should contain only one such instance.
 ;; If there is more than one, they won't work right.
 '(rainbow-delimiters-base-face ((t (:inherit nil :foreground "black" :weight extra-bold :height 1.0))))
 '(rainbow-delimiters-depth-1-face ((((class color) (min-colors 89)) (:bold t :foreground "#cc0000"))))
 '(rainbow-delimiters-depth-2-face ((((class color) (min-colors 89)) (:bold t :foreground "#1f5bff"))))
 '(rainbow-delimiters-depth-3-face ((((class color) (min-colors 89)) (:bold t :foreground "#00af00"))))
 '(rainbow-delimiters-depth-4-face ((t (:inherit rainbow-delimiters-base-face :foreground "dodger blue" :weight extra-bold :height 1.0))))
 '(rainbow-delimiters-depth-5-face ((t (:inherit rainbow-delimiters-base-face :foreground "violet red" :weight extra-bold))))
 '(rainbow-delimiters-depth-6-face ((t (:inherit rainbow-delimiters-base-face :foreground "red1")))))

(setq inhibit-startup-message t)
(tool-bar-mode -1)
(setq line-move-visual nil)
(when (version<= "26.0.50" emacs-version )
  (global-display-line-numbers-mode))
(require 'package)
(package-initialize)
(add-to-list 'package-archives
	     '("marmalade" . "http://marmalade-repo.org/packages/"))
(add-to-list 'package-archives
	     '("melpa" . "http://melpa.milkbox.net/packages/") t)

(require 'org-bullets)
(add-hook 'org-mode-hook 'org-bullets-mode)

(require 'rainbow-delimiters)
(add-hook 'prog-mode-hook 'rainbow-delimiters-mode)

(require 'ivy)
(ivy-mode 1)
(setq ivy-use-virtual-buffers t)
(setq ivy-count-format "(%d/%d) ")

(require 'which-key)
(which-key-mode)

(require 'auto-complete)
(ac-config-default)

(require 'flycheck)
(add-hook 'after-init-hook #'global-flycheck-mode)
(add-hook 'c++-mode-hook (lambda () (setq flycheck-gcc-language-standard "c++17")))

(defun my-flycheck-rtags-setup ()
  "Configure flycheck-rtags for better experience."
  (flycheck-select-checker 'rtags)
  (setq-local flycheck-check-syntax-automatically nil)
  (setq-local flycheck-highlighting-mode nil))
(add-hook 'c++-mode-hook #'my-flycheck-rtags-setup)


(require 'rtags)
(require 'company)
(require 'company-rtags)
(require 'flycheck-rtags)
(setq rtags-autostart-diagnostics t)
(rtags-diagnostics)
(setq rtags-completions-enabled t)
(eval-after-load 'company
  '(add-to-list
    'company-backends 'company-rtags))

(rtags-enable-standard-keybindings)
(define-key c-mode-base-map (kbd "M-.") 'rtags-find-symbol-at-point)
(define-key c-mode-base-map (kbd "M-,") 'rtags-location-stack-back)
(define-key c-mode-base-map (kbd "M-;") 'rtags-find-file)
(define-key c-mode-base-map (kbd "C-.") 'rtags-find-symbol)
(define-key c-mode-base-map (kbd "C-,") 'rtags-find-references)
(define-key c-mode-base-map (kbd "C-<") 'rtags-find-virtuals-at-point)
(define-key c-mode-base-map (kbd "M-i") 'rtags-imenu)

(defun insert-inventory ()
  (interactive)
  (save-excursion)
  (insert "| Affects                | How it affects instinct |
|------------------------+-------------------------|
| Pride                  |                         |
| Self Esteem            |                         |
| Personal Relationships |                         |
| Companionship          |                         |
| Prestige               |                         |
| Material Security      |                         |
| Emotional Security     |                         |
| Accepted Sex           |                         |
| Unaccepted Sex         |                         |
| Social Ambitions       |                         |
| Security Ambitions     |                         |
| Sex Ambitions          |                         |"))
(global-set-key (kbd "C-c i") 'insert-inventory)

(require 'cc-mode
	 (define-key c-mode-base-map (kbd "RET") 'newline-and-indent)
	 (setq c-default-style "linux"
          c-basic-offset 4))
(provide 'init)
;;; init.el ends here
